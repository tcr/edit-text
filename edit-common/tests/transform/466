            DocChars(" "), DocGroup({"tag": "caret", "client": "b"}, []),
            DocChars("pFfFG 7QjF")
        ]),
        DocGroup({"tag": "h1"}, [
            DocChars("0"), DocGroup({"tag": "caret", "client": "a"}, []),
            DocChars("Hello CG What\'s up with you?")
        ])
    ])
]

a_del: [
    DelGroup([
        DelWithGroup([
            DelSkip(1), DelWithGroup([]),
            DelSkip(10)
        ]),
        DelWithGroup([
            DelChars(1), DelGroup([]),
            DelChars(2)
        ])
    ])
]
