    AddWithGroup([
        AddSkip(1), AddWithGroup([]),
        AddSkip(10)
    ]),
    AddWithGroup([
        AddChars("O"), AddGroup({"tag": "caret", "client": "a"}, []),
        AddChars("p")
    ])
]

