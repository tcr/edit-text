    AddWithGroup([
        AddGroup({"tag": "pre"}, [
            AddSkip(2), AddGroup({"tag": "caret", "client": "a"}, []),
            AddChars(" "), AddGroup({"tag": "caret", "client": "b"}, []),
            AddChars("S "), AddSkip(18)
        ])
    ])
]

