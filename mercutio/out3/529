    DelGroup([
        DelWithGroup([
            DelWithGroup([]),
            DelSkip(2)
        ]),
        DelWithGroup([
            DelSkip(1), DelWithGroup([]),
            DelSkip(10), DelChars(1)
        ]),
        DelWithGroup([
            DelGroup([])
        ])
    ])
]
